        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(bEnableDraw)
{
    // Draw to Surface
    if(surface_exists(TextureSurface))
    {
        surface_set_target(TextureSurface);

        draw_clear_alpha(c_black, 0.0);
        d3d_set_projection_ortho(0,0,DrawResolution,DrawResolution,0);
        
        draw_set_alpha(1);
        draw_set_color(c_white);
        
        /*
            DRAW CODE FOR VISCOUS SUBSTANCE ENEMY
        */
        if((FlickerTimer) % 2  == 0){  
            
            //BodyOffsets
            var bodyYOff = sin(BobTimer/20) * 4;
            var bodyXScl = sin(BobTimer/20) * 0.025;
            var bodyYScl = cos(BobTimer/20) * 0.025;       
            
            //Wings
            var wingYOff = sin(BobTimer/10) * 6;
            var wingXOff = cos(BobTimer/10) * 6; 
            var wingRot = 4 + Scr_SCurve(sin(BobTimer/20)) * 4;
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_LWing, 0, DrawResolution/2 + wingXOff, 60 + bodyYOff - wingYOff, 1.0, 1.0, wingRot, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_RWing, 0, DrawResolution/2 - wingXOff, 60 + bodyYOff - wingYOff, 1.0, 1.0, -wingRot, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // Body                     
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_Body, 0, DrawResolution/2, 190 + bodyYOff, 1.0 + bodyXScl, 1.0 + bodyYScl, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // HindLegs
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_HindLegs, 0, DrawResolution/2, DrawResolution, 1.0 + bodyXScl, 1.0 + bodyYScl, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // Forelimbs
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_Forelimbs, 0, DrawResolution/2, DrawResolution, 1.0 + bodyXScl, 1.0 + bodyYScl, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // Head
            var HeadXOff = sin(BobTimer/8) * 8;
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_Head, 0, DrawResolution/2 + HeadXOff, 90 - bodyYOff, 1.0, 1.0, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // Eyes
            var MouthSquish = sin(MouthTimer/10) * 0.25;
            if(Eyes % 3  == 0)
                draw_sprite_ext(Spr_BossEnemy_Gargoyle_Eyes, 0, DrawResolution/2 + HeadXOff, 86 - bodyYOff, 1.0, 1.0 - MouthSquish, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
            
            // Mouth
            draw_sprite_ext(Spr_BossEnemy_Gargoyle_Mouth, 0, DrawResolution/2 + HeadXOff, 115 - bodyYOff, 1.0, 1.0 + MouthSquish, 0.0, c_white, DeathAnimTimer/DeathAnimMaxTimer);
        }    
        
        surface_reset_target();
        Scr_Reset3dView();
    }else{
        TextureSurface = surface_create(DrawResolution,DrawResolution);
    }
    
    event_inherited();
}

</string>
