    Scr_DrawText(string(Obj_PlayerData.Hitpoints) + "/" + string(Obj_PlayerData.MaxHitpoints), 63 - PlayerStatsAnimOffset, 318, 0.8, GUI_Font_OldStyleKana); 
    
